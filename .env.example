# Environment Variables (example)
# Copy this file to .env and fill in your values

# 대전 공공데이터 API 키 (https://www.data.go.kr 에서 발급)
VITE_API_KEY=your_api_key_here

# API 베이스 URL
VITE_API_BASE_URL=https://apis.data.go.kr/6300000/openapi2022

# 카카오맵 JavaScript API 키
# https://developers.kakao.com/ 에서 발급
# 플랫폼 키 > JavaScript 키 > JS SDK 도메인에 사이트 도메인 등록 필요
VITE_KAKAO_MAP_KEY=your_kakao_map_js_key_here

# Supabase 설정
# https://supabase.com/dashboard 에서 프로젝트 생성 후
# Settings > API > Project URL, anon public 키 확인
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
