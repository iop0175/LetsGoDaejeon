    //자동완성 스크립트
    function onSearch(type,keycode) {
        var code = keycode.keyCode; // 키보드 이벤트 키 값

        var nLenth = $("#searchTopForm input[name=query]").val().trim().length;
        if (nLenth != '') {

            var len;

            len = $('#wordList > li').length; // 검색된 리스트 수

            console.log("len = "+len)
            if (len > 0) { // 검색된 리스트가 하나라도 있는 경우
                if (code != 40 && code != 38) { // ↓ 키 눌렀을 때, ↑ 키 눌렀을 때
                    onAutoSearch();
                }
            } else {
                if (27 && code != 40 && code != 38) { // ↓ 키 눌렀을 때, ↑ 키 눌렀을 때
                    onAutoSearch();
                }
            }
        } else {

            $('.auto-complete').removeClass('on');

        }
    }

    function onAutoSearch(){

        var query = $("#searchTopForm input[name=query]").val();
        console.log("query = "+query)
        $.ajax({
            type: "POST",
            url: "/search/autoSearch.do",
            contentType : "application/x-www-form-urlencoded; charset=UTF-8",
            dataType:'json',
            data: { "query" : query},
            success: function(jsonDatas) {
                var responsestatus = jsonDatas.resultSet.errorCode;
                if(responsestatus == 0){
                    var result = jsonDatas.resultSet.result[0];
                    $(result).each(function(r,resultEntry){

                        var totalCnt =  resultEntry.totalSize;
                        if(totalCnt > 0){
                            var items = resultEntry.resultDocuments;
                            var str = "";
                            $(items).each(function(i,itemEntry){
                                var keyword = itemEntry.KEYWORDS;
                                keyword = keyword.replace(query, "<mark>"+query+"</mark>");
                                str += "<li class=\"find\"><a href=\"#\" onclick=\"javascript:goSearchList('" + itemEntry.KEYWORDS + "');\">"+keyword+"</a></li>";
                            });
                            $('.auto-complete').addClass('on');
                            $("#wordList").html(str);
                        }else{
                            $("#wordList").html('');
                        }
                    });

                }

            }
            , error : function( jqXHR, textStatus, errorThrown ) {
                console.log( textStatus );
                console.log( errorThrown );
            }
            ,beforeSend:function(){
            }
            ,complete:function(){
            }
        });
    }

