                    return false;
                }
            },
            error:function(){
                alert("err");
            }
        });
    }

    // 스크랩 체크 해지
    function updateBookmark(division,recommendIdx, pId, lId){
        $.ajax({
            url:'/recommend/recommendBookmarkUpdate.do',
            method:'post',
            dataType:'json',
            contentType : "application/x-www-form-urlencoded; charset=UTF-8",
            data:'recommendIdx=' + recommendIdx + '&type=bookmark&division=' + division,
            success:function(entry){
                if(entry[0].success == "login"){
                    alert('로그인 후 이용 가능 합니다.');
                    return false;
                    //location.href = "/member/loginMain.do";
                }else if(entry[0].success == "ent"){
                    alert('스크랩은 개인회원만 사용 가능 합니다.');
                    return false;
                }else if(entry[0].success == "delSuccess"){
                    alert('스크랩이 해지 되었습니다.');
                    $("#"+pId).removeClass("ico-fav").addClass("ico-fav-off").attr("title", "");
                    $("#"+lId).removeClass("ico-fav").addClass("ico-fav-off").attr("title", "");
                    // $("#"+pId).removeClass("active");
                    // $("#"+lId).removeClass("active");
                    return false;
                }else if(entry[0].success == "success"){
                    alert('스크랩이 완료 되었습니다.');
                    $("#"+pId).removeClass("ico-fav-off").addClass("ico-fav").attr("title", "선택됨");
                    $("#"+lId).removeClass("ico-fav-off").addClass("ico-fav").attr("title", "선택됨");
                    // $("#"+pId).addClass("active");
                    // $("#"+lId).addClass("active");
                    return false;
                }
            },
            error:function(){
                alert("err");
            }
        });
    }

