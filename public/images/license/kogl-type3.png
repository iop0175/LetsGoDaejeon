    // 다이퀘스트 기관검색
    function selDept(dept ,orgCode) {

        if($('input[type="checkbox"][value="' + orgCode + '"]').is(':checked')) {
            var nmValue = $('input[name="company"]').val()
            var codeValue = $('input[name="orgCode"]').val()
            $(".site-list").show()

            if (dept == "") {
                $(".site-list").append("<li data-name=\"\" data-code=\"\"><button type=\"button\" class=\"i-del\" aria-label=\"삭제\"><span>전체기관</span><i class=\"ico-del-wh\" role=\"img\"></i></button></li>");
            } else {
                $(".site-list").append("<li data-name=\"" + dept + "\" data-code=\"" + orgCode + "\"><button type=\"button\" class=\"i-del\" aria-label=\"삭제\"><span>" + dept + "</span><i class=\"ico-del-wh\" role=\"img\"></i></button></li>");
                $("input[name=company]").val(nmValue ? nmValue+','+dept : dept);
            }
            if (orgCode == "") {
                $("input[name=orgCode]").val("");
            } else {
                $("input[name=orgCode]").val(codeValue ? codeValue+','+orgCode : orgCode);
            }

            if (dept == "" && orgCode == "") {
                $(".site-list__search").removeClass("on");
                $(".sh-detail-pop").attr('tabindex', 0).removeClass("dark");
            }
        }else{
            $(".site-list li[data-code='" + orgCode + "']").remove();

            var nmValue = $('input[name="company"]').val().split(',');
            var codeValue = $('input[name="orgCode"]').val().split(',');

            var updatedNmValue = nmValue.filter(function(value) {
                return value !== dept;
            }).join(',');

            var updatedCodeValue = codeValue.filter(function(value) {
                return value !== orgCode;
            }).join(',');

            $('input[name="company"]').val(updatedNmValue);
            $('input[name="orgCode"]').val(updatedCodeValue);
            if ($("#orgCode").val() == '') {
                $(".site-list").hide()
            }
        }
    }

    function deptPagingView(startNum, totalcount, li) {

        var pagingHTML 		= "";

        var totalcount		= totalcount;
        var resultCnt		= 12;
        var anchorSacle     = 5;
        var m_before = -1;
        var m_next = -1;
        var m_bundleBefore = -1;
        var m_bundleNext = -1;
        var m_firstPage = -1;
        var m_lastPage = -1;
        var m_totalPgCount = 0;
        var m_totlalBundlePgCount = 0;
        var m_pageCount = 0;
        var m_curPageNumber = 1;
        var m_pages =[];
        m_pages[0] = ["1","-1"];
        var curBunbleNum = parseInt(startNum / (resultCnt * anchorSacle));
        var totalPageCnt = parseInt(totalcount / resultCnt);

        if(totalcount % resultCnt > 0) {
            totalPageCnt++;
        }

        m_totalPgCount = totalPageCnt;

        if ( startNum > 0){        // 이전 페이지
            var beforePg = startNum - resultCnt;
            m_before = beforePg;
        }

        if( startNum+resultCnt < totalcount ){        // 다음페이지
            var nextPg = startNum + resultCnt;
            m_next = nextPg;
        }

        //번들 뒤로 이동
        var bunbleBeforePg = (curBunbleNum-1) * resultCnt * anchorSacle;    //이전 번들로 이동하는 번호
        if(curBunbleNum > 0){
            m_bundleBefore = bunbleBeforePg;
        }

        //번들 앞으로 이동
        var bundleNextPg = (1 + curBunbleNum) * anchorSacle * resultCnt;
        if( totalPageCnt >= anchorSacle && (curBunbleNum+1) * anchorSacle <  totalPageCnt ){
            m_bundleNext = bundleNextPg;
        }

        //맨처음..
        if(startNum != 0 && curBunbleNum != 0){
            m_firstPage = 0;
        }
        //맨끝...
        var lastPage = parseInt((resultCnt * totalPageCnt) - resultCnt) ;
        if( totalPageCnt >= anchorSacle && (curBunbleNum+1) * anchorSacle <  totalPageCnt ) {
            m_lastPage = lastPage;
        }

        var pageCount = 1;
        var pages = [];
        for(var i = 0; i < anchorSacle; i++) {
            var startCnt = ((curBunbleNum * anchorSacle) + i) * resultCnt;
            var pageNum = (curBunbleNum * anchorSacle) + i + 1;

            if(startCnt < totalcount) {
                if (startCnt != startNum) {
                    pages[i] = [pageNum,startCnt];
                } else {
                    pages[i] = [pageNum,"-1"];
                    m_curPageNumber = pageNum;
                }
                m_pageCount = pageCount;
                pageCount++;
            }
        }
        m_pages = pages;

        /////////////////////////////////////////////////////////////////////////////

        var str = "<ul class=\"pagination pagination2 pagination-centered\">";
        if(m_bundleBefore != -1) {
            str += "<li class=\"i first \"><a href=\"javascript:void(0);\" onClick=\"javascript:getDept('"+m_firstPage+"','" + $("#chosung").val()+ "','" + li+ "');\"><span class=\"s\"><span>처음 <span class=\"t\">목록으로 이동</span></span></span></a></li>";

        } else {
            str += "<li class=\"i first disabled\"><span class=\"s\"><span>처음 <span class=\"t\">목록</span></span></span></li>";
            // str += "<li class=\"i first \"><a href=\"#\"><span class=\"s\"><span>처음 <span class=\"t\">목록으로 이동</span></span></span></a></li>";
        }
        if(m_firstPage != -1) {
            str += "<li class=\"i prev \"><a href=\"javascript:void(0);\" onClick=\"javascript:getDept('"+m_bundleBefore+"','" + $("#chosung").val()+ "','" + li+ "','"+((m_bundleBefore/12)+1)+"');\"><span class=\"s\"><span>이전 <span class=\"t\">목록으로 이동</span></span></span></a></li>";
        } else {
            str += "<li class=\"i prev \"><span class=\"s\"><span>이전 <span class=\"t\">목록</span></span></span></li>";
            // str += "<li class=\"i prev \"><a href=\"#\"><span class=\"s\"><span>이전 <span class=\"t\">목록으로 이동</span></span></span></a></li>";
        }

        var pageCount = m_pageCount;
        var v_pages = m_pages;

        for(var i=0; i<pageCount && i < v_pages.length; i++) {
            if(v_pages[i][1]==-1) {
                str += "<li class=\"on\"><span><em title=\"현재목록\"><span>"+v_pages[i][0]+"</span></em></span></li>";
                // str += "<li class=\"active\"><a href=\"#\" title=\"현재위치\"><span>"+v_pages[i][0]+"</span></a></li>";
            } else {
                str += "<li><span><a href=\"javascript:void(0);\" onClick=\"javascript:getDept('"+v_pages[i][1]+"','" + $("#chosung").val()+ "','" + li+ "','"+v_pages[i][0]+"');\"><span>"+v_pages[i][0]+"</span></a></span></li>";
                // str += "<li><a href=\"javascript:void(0);\" onClick=\"javascript:getDept('"+v_pages[i][1]+"','" + $("#chosung").val()+ "','" + li+ "');\"><span>"+v_pages[i][0]+"</span></a></li>";
            }
        }

        if(m_bundleNext != -1) {
            str += "<li class=\"i next\"><a href=\"javascript:void(0);\" onClick=\"javascript:getDept('"+m_bundleNext+"','" + $("#chosung").val()+ "','" + li+ "','"+((m_bundleNext/12)+1)+"')\"><span class=\"s\"><span>다음 <span class=\"t\">목록으로 이동</span></span></span></a></li>";
        } else {
            str += "<li class=\"i next \"><span class=\"s\"><span>다음 <span class=\"t\">목록</span></span></span></li>";
            // str += "<li class=\"i next\"><a href=\"#\"><span class=\"s\"><span>다음 <span class=\"t\">목록으로 이동</span></span></span></a></li>";
        }
        if(m_lastPage != -1) {
            str += "<li class=\"i end\"><a href=\"javascript:void(0);\" onClick=\"javascript:getDept('"+m_lastPage+"','" + $("#chosung").val()+ "','" + li+ "','"+((m_lastPage/12)+1)+"')\"><span class=\"s\"><span>끝 <span class=\"t\">목록으로 이동</span></span></span></a></li>";
        } else {
            str += "<li class=\"i end \"><span class=\"s\"><span>끝 <span class=\"t\">목록</span></span></span></li>";
            // str += "<li class=\"i end\"><a href=\"#\"><span class=\"s\"><span>끝 <span class=\"t\">목록으로 이동</span></span></span></a></li>";
        }
        str += " </ul></div>";
        /////////////////////////////////////////////////////////////////////////////
//	alert(str);

        $(".paginationDeptSet").html(str);
    }


