{
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/rss.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/rss+xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=604800"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap.xml"
    },
    {
      "source": "/sitemap-main.xml",
      "destination": "/api/sitemap-main.xml"
    },
    {
      "source": "/sitemap-travel.xml",
      "destination": "/api/sitemap-travel.xml"
    },
    {
      "source": "/sitemap-food.xml",
      "destination": "/api/sitemap-food.xml"
    },
    {
      "source": "/sitemap-accommodation.xml",
      "destination": "/api/sitemap-accommodation.xml"
    },
    {
      "source": "/sitemap-leisure.xml",
      "destination": "/api/sitemap-leisure.xml"
    },
    {
      "source": "/sitemap-culture.xml",
      "destination": "/api/sitemap-culture.xml"
    },
    {
      "source": "/sitemap-shopping.xml",
      "destination": "/api/sitemap-shopping.xml"
    },
    {
      "source": "/sitemap-festival.xml",
      "destination": "/api/sitemap-festival.xml"
    },
    {
      "source": "/trip/shared/:tripId",
      "destination": "/shared-trip/:tripId"
    }
  ]
}
